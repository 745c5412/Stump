<Window x:Class="WorldEditor.Editors.Files.D2O.EditorDialog"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="EditorDialog" Height="300" Width="300" DataContext="{Binding RelativeSource={RelativeSource Self}}">
  <Grid Margin="10">
    <Grid.RowDefinitions>
      <RowDefinition Height="*" />
      <RowDefinition Height="Auto" />
    </Grid.RowDefinitions>

    <ContentControl Name="Container" >
    </ContentControl>

    <StackPanel Orientation="Horizontal" Grid.Row="1" HorizontalAlignment="Right" Margin="5">
      <Button Width="75" Margin="2" Click="OkButton_Click" IsDefault="True">OK</Button>
      <Button Width="75" Margin="2" Click="CancelButton_Click" IsCancel="True">Cancel</Button>
    </StackPanel>

  </Grid>
</Window>
