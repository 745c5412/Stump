<?xml version="1.0" encoding="utf-8"?>

<Configuration xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="world_config.xsd">
  <Stump.Server.BaseServer>
    <Settings>
      <EnableBenchmarking>true</EnableBenchmarking>
    </Settings>
    <Database>
      <DatabaseAccessor>
        <LoginUser>root</LoginUser>
        <LoginPassword></LoginPassword>
        <LoginHost>localhost</LoginHost>
        <DatabaseName>stump_world</DatabaseName>
      </DatabaseAccessor>
    </Database>
    <Network>
      <MessageListener>
        <Host>127.0.0.1</Host>
        <Port>3467</Port>
        <MaxConcurrentConnections>2000</MaxConcurrentConnections>
        <MaxPendingConnections>100</MaxPendingConnections>
      </MessageListener>
      <ClientVersion>
        <Severity>Medium</Severity>
        <VersionRequired>2.2.3.36341.1</VersionRequired>
        <ActualVersion>1280</ActualVersion>
        <RequiredVersion>1285</RequiredVersion>
      </ClientVersion>
      <WorkerManager>
        <AutoWorkerNumber>true</AutoWorkerNumber>
        <WorkerThreadNumber>2</WorkerThreadNumber>
      </WorkerManager>
    </Network>
  </Stump.Server.BaseServer>

  <Stump.Server.WorldServer>
    <WorldServer>
      <Testarray>"1, 2, 3", "4, 5, 6"</Testarray>
      <ServerId>1</ServerId>
      <ServerName>Jiva</ServerName>
      <ServerAddress>localhost</ServerAddress>
      <ServerPort>3467</ServerPort>
    </WorldServer>
  </Stump.Server.WorldServer>
</Configuration>