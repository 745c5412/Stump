<?xml version="1.0" encoding="utf-8"?>

<Configuration xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="auth_config.xsd">
  <Stump.Server.BaseServer>
    <Settings>
      <EnableBenchmarking>true</EnableBenchmarking>
    </Settings>
    <Database>
      <DatabaseAccessor>
        <LoginUser>root</LoginUser>
        <LoginPassword></LoginPassword>
        <LoginHost>localhost</LoginHost>
        <DatabaseName>stump_auth</DatabaseName>
      </DatabaseAccessor>
    </Database>
    <Network>
      <MessageListener>
        <Host>127.0.0.1</Host>
        <Port>5555</Port>
        <MaxConcurrentConnections>2000</MaxConcurrentConnections>
        <MaxPendingConnections>100</MaxPendingConnections>
      </MessageListener>
      <ClientVersion>
        <Severity>Medium</Severity>
        <VersionRequired>2.3.0.37782.1</VersionRequired>
        <ActualVersion>1304</ActualVersion>
        <RequiredVersion>1304</RequiredVersion>
      </ClientVersion>
      <WorkerManager>
        <AutoWorkerNumber>true</AutoWorkerNumber>
        <WorkerThreadNumber>2</WorkerThreadNumber>
      </WorkerManager>
    </Network>
  </Stump.Server.BaseServer>
</Configuration>